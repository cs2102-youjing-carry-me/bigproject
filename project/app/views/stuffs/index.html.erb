<p id="notice"><%= notice %></p>

<h1>Stuffs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th>Pickup location</th>
      <th>Return location</th>
      <th>Availability</th>
      <th>Available date</th>
      <th>Bidding endtime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stuffs.each do |stuff| %>
      <tr>
        <td><%= stuff.name %></td>
        <td><%= stuff.user %></td>
        <td><%= stuff.pickup_location %></td>
        <td><%= stuff.return_location %></td>
        <td><%= stuff.availability %></td>
        <td><%= stuff.available_date %></td>
        <td><%= stuff.bidding_endtime %></td>
        <td><%= link_to 'Show', stuff %></td>
        <td><%= link_to 'Edit', edit_stuff_path(stuff) %></td>
        <td><%= link_to 'Destroy', stuff, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stuff', new_stuff_path %>
